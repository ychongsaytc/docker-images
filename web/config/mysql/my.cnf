
[client]
port             = 3306
socket           = /tmp/mysqld.sock

[mysqld_safe]
pid-file         = /tmp/mysqld.pid
socket           = /tmp/mysqld.sock
nice             = 0

[mysqld]
skip-host-cache
skip-name-resolve
user             = mysql
pid-file         = /tmp/mysqld.pid
socket           = /tmp/mysqld.sock
port             = 3306
basedir          = /usr
datadir          = /var/lib/mysql
tmpdir           = /tmp
lc-messages-dir  = /usr/share/mysql
explicit_defaults_for_timestamp

#bind-address    = 0.0.0.0

#log-error        = /var/log/mysql/error.log

symbolic-links   = 0

